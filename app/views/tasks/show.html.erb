<div class="example">
        
    <p class="solid">Title:<%= @task.title %></p>
    <p class="solid">Expire:<%= @task.date %></p>
   
</div>

<p class="solid">
    Memo:<%= @task.memo %>
</p>

<div class="example">
    <%= link_to 'Return', :back %>
    <% if current_user %>
        <%= button_to 'delete', task_path(@task), method: :delete %>
        <%= link_to 'edit', edit_task_path(@task) %>
    <% end %>
</div>

